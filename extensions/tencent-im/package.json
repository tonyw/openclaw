{
  "name": "@openclaw/tencent-im",
  "version": "1.0.0",
  "description": "Tencent Cloud IM channel plugin for OpenClaw",
  "type": "module",
  "main": "index.ts",
  "scripts": {},
  "dependencies": {
    "tim-js-sdk": "^2.27.6",
    "tim-upload-plugin": "^1.0.4"
  },
  "devDependencies": {
    "openclaw": "workspace:*"
  },
  "peerDependencies": {
    "openclaw": "workspace:*"
  },
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "tencent-im",
      "label": "Tencent IM",
      "selectionLabel": "Tencent Cloud IM (腾讯云IM)",
      "docsPath": "/channels/tencent-im",
      "docsLabel": "tencent-im",
      "blurb": "Tencent Cloud instant messaging service via REST API.",
      "aliases": [
        "tim",
        "tencent"
      ],
      "order": 80,
      "quickstartAllowFrom": true
    },
    "install": {
      "npmSpec": "@openclaw/tencent-im",
      "localPath": "extensions/tencent-im",
      "defaultChoice": "local"
    }
  }
}
